{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 800,
  "tabWidth": 4,
  "plugins": ["prettier-plugin-go-template", "prettier-plugin-tailwindcss"],
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "go-template",
      },
    },
  ]
}
